{
  "name": "prx-sample",
  "version": "0.1.0",
  "scripts": {
    "buildinfo": "ts-node prepare.build-info.ts",
    "ng": "ng",
    "prestart": "npm run buildinfo",
    "start": "ng serve",
    "prebuild": "npm run buildinfo",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "build:oms": "ng build oms",
    "build:ui-blocks": "ng build ui-blocks",
    "test": "ng test",
    "test:once": "ng test oms --browsers ChromeHeadless --watch false",
    "lint": "ng lint",
    "lint:fix": "npm run lint -- --fix",
    "lint-staged": "git add . && npx lint-staged",
    "beautify:all": "npx js-beautify --type html -r --config .jsbeautifyrc --file '{apps,libs}/**/*.html'",
    "prettier:all": "npx prettier --write '{apps,libs}/**/*.ts'",
    "bump-version": "npx bump --commit 'Release v%s'",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.3",
    "@angular/cdk": "^16.2.3",
    "@angular/common": "^16.2.3",
    "@angular/compiler": "^16.2.3",
    "@angular/core": "^16.2.3",
    "@angular/forms": "^16.2.3",
    "@angular/material": "^16.2.3",
    "@angular/platform-browser": "^16.2.3",
    "@angular/platform-browser-dynamic": "^16.2.3",
    "@angular/router": "^16.2.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular-eslint/builder": "16.1.1",
    "@angular-eslint/eslint-plugin": "16.1.1",
    "@angular-eslint/eslint-plugin-template": "16.1.1",
    "@angular-eslint/schematics": "16.1.1",
    "@angular-eslint/template-parser": "16.1.1",
    "@angular/cli": "~16.2.1",
    "@angular/compiler-cli": "^16.2.3",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jasmine-core": "~4.6.0",
    "js-beautify": "^1.14.9",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^14.0.1",
    "ng-packagr": "^16.2.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^15.0.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.3"
  }
}
